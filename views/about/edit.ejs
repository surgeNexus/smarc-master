<% include ../partials/header %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">Edit Document</h1>
        <div>
            <form action="/about/aboutcollection/<%= foundAbout._id %>?_method=PUT" method="POST">
                <div class="form-group">
                    Meeting Location URL
                    <input class="form-control" type="text" name="meeting" value="<%= foundAbout.meeting %>">
                </div>
                <div class="form-group">
                    Field Day Location URL
                    <input class="form-control" type="text" name="fieldDay" value="<%= foundAbout.fieldDay %>">
                </div>
                <div class="form-group">
                    <h5>VE Testing Dates</h5>
                    <textarea class="form-control" type="text" name="test" id="htmeditor"><%- foundAbout.test %></textarea><script src="https://htmeditor.com/js/htmeditor.min.js" htmeditor_textarea="htmeditor" full_screen="no" editor_height="480" run_local="no"> </script> 
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit</button>
                </div>
            </form>
            <a href="/about/">Go Back</a>
        </div>
    </div>
</div>
<% include ../partials/footer %>
