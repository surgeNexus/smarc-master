<div class="container position-relative" style="z-index: 1;">
    <div class="position-absolute top-0 start-50 translate-middle on-top">
      <% if(error && error.length > 0){ %>
        <br><br><br><br>
          <div class="alert alert-danger on-top shadow" id="flash-message" role="alert">
              <%= error %>
          </div>
      <% } %>
      <% if(success && success.length > 0){ %>
        <br><br><br><br>
          <div class="alert alert-success on-top shadow" id="flash-message" role="alert">
              <%= success %>
          </div>
      <% } %>
    </div>
  </div>
  
  <script>
    setTimeout(function() { 
        $('#flash-message').fadeOut('fast').empty(); 
    }, 5000); 
  </script>