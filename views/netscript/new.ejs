<% include ../partials/adminheader %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">New NCS</h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/info/netscript/" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="date" placeholder="Date">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="name" placeholder="Name">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="callsign" placeholder="Callsign">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit</button>
                </div>
            </form>
            <a href="/info/netscript/netscriptcollection">Go Back</a>
        </div>
    </div>
</div>
