<% include ../partials/header %>

<div class="page-center">
    <div class="container form-media">
        <div class="row well well-lg text-center">
        <% foundOfficers.forEach(function(officer){ %>
            <div class="col-md-4 col-sm-6">
                <div class="thumbnail">
                   <div class="caption">
                        <h3><%= officer.title %></h3>
                        <img src="<%= officer.pictureLoc %>">
                        <h4><%= officer.name %></h4>
                        <h4><%= officer.callsign %></h4>
                   </div>
                </div>
            </div>
        <% }); %>
        </div>
    </div>
</div>
<% include ../partials/footer %>
