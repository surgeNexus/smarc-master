<% include ../partials/header %>

<div class="page-center">
    <div class="container">
        <div class="row">
                <div class="col-md-12">
                        <div>
                                <h1 class="title-center">Silent Key Memorial</h1>
                                <hr>
                        </div>
                </div>
        </div>
        <div class="form-media">
            <div class="row text-center well well-lg" style="display:flex; flex-wrap: wrap;">
                <% foundSk.reverse().forEach(function(sk){ %>
                <div class="col-md-3 col-sm-6">
                    <div class="thumbnail">
                       <div class="caption">
                           <h4><%= sk.name %></h4>
                           <h5><%= sk.callsign %></h5>
                       </div>
                    </div>
                </div>
                <% }); %>
            </div>
        </div>
    </div>
</div>

<% include ../partials/footer %>